{
  "name": "mgk-layouts",
  "version": "0.0.1",
  "description": "",
  "main": "app.js",
  "dependencies": {
    "lodash": "^4.17.11",
    "nunjucks": "^3.2.0",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "fast-check": "^1.16.0",
    "npm-watch": "^0.6.0",
    "tap": "^14.4.1"
  },
  "watch": {
    "test": {
      "patterns": ["app", "src", "test"],
      "extensions": "js,njk"
    },
    "serve": {
      "patterns": ["."],
      "extensions": "js,njk",
      "ignore": ["test/"]
    }
  },
  "scripts": {
    "test": "node_modules/.bin/tap -Rspec --test-ignore=test/victor-assert.js",
    "watch-tests": "npm-watch test",
    "serve": "node app.js",
    "watch-serve": "npm-watch serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://petekneller@github.com/petekneller/mgk-layouts.git"
  }
}
